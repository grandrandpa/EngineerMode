<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="textSize" format="dimension"/>
    <attr name="title" format="string"/>

    <declare-styleable name="MenuButton">
        <attr name="text" format="string"/>
        <attr name="drawable" format="reference"/>
        <attr name="textSize"/>
    </declare-styleable>
    <declare-styleable name="ItemGroup">
        <attr name="optArray" format="reference"/>
        <attr name="title"/>
        <attr name="textSize"/>
    </declare-styleable>
    <declare-styleable name="ProgressGroup">
        <attr name="progressStyle" format="reference"/>
        <attr name="thumbDrawable" format="reference"/>
        <attr name="title"/>
        <attr name="textSize"/>
    </declare-styleable>
    <declare-styleable name="SwitchItem">
        <attr name="switchDrawable" format="reference"/>
        <attr name="subtitle" format="string"/>
        <attr name="statusText" format="string"/>
        <attr name="ip" format="string"/>
        <attr name="title"/>
        <attr name="textSize"/>
    </declare-styleable>
</resources>